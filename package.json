    "@types/sequelize": "^4.28.20",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
